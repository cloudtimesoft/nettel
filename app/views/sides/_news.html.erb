 <p xmlns="http://www.w3.org/1999/html">新闻　关键字news</p>



<div class="col-md-9 fluid-height clearfix" >
  <div class="widget-container fluid-height clearfix" >

    <% @sidecontents.each do |sidecontent| %>
        <tr>

          <div class="reviewer-info">
          &nbsp;&nbsp;<td>
            <i class="icon-double-angle-right"></i>
            <a href="/sides/<%=sidecontent.id%>"data-no-turbolink="true"><%= sidecontent.title %></a>
          </td>
            <div  class="pull-right" style="color: #888888">发布日前</span>&nbsp;<%= sidecontent.time.strftime("%Y-%m-%d") %>&nbsp;&nbsp;</div>
          </div>
          <div class="review-text">
            <p style="text-align: left;margin: 20px">
             <span><%= sidecontent.inpaper%></span>
            </p>
          </div>

        <p style="border-bottom: 1px  dashed #d2d2d2 ;margin: 10px"></p>
        </tr>
    <% end %>



</div>


</div>

<div class="col-md-3 fluid-height clearfix">



  <div class="widget-container fluid-height clearfix" style="margin-bottom: 20px">


    <%=render 'right_top'%>


  </div>
  <div class="widget-container fluid-height clearfix"style="margin-bottom: 20px">
    <%=render 'right_middle'%>
  </div>
  <div class="widget-container fluid-height clearfix">
    <%=render 'right_bottom'%>
  </div>

</div>