<h1>Listing admin_role_refs</h1>

<table>
  <thead>
    <tr>
      <th>Admin</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_role_refs.each do |admin_role_ref| %>
      <tr>
        <td><%= admin_role_ref.admin_id %></td>
        <td><%= admin_role_ref.role_id %></td>
        <td><%= link_to 'Show', admin_role_ref %></td>
        <td><%= link_to 'Edit', edit_admin_role_ref_path(admin_role_ref) %></td>
        <td><%= link_to 'Destroy', admin_role_ref, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin role ref', new_admin_role_ref_path %>
