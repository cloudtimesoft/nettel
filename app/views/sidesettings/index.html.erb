<h1>Listing sidesettings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Logo</th>
      <th>Tel</th>
      <th>Address</th>
      <th>Icp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sidesettings.each do |sidesetting| %>
      <tr>
        <td><%= sidesetting.name %></td>
        <td><%= sidesetting.logo %></td>
        <td><%= sidesetting.tel %></td>
        <td><%= sidesetting.address %></td>
        <td><%= sidesetting.icp %></td>
        <td><%= link_to 'Show', sidesetting %></td>
        <td><%= link_to 'Edit', edit_sidesetting_path(sidesetting) %></td>
        <td><%= link_to 'Destroy', sidesetting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sidesetting', new_sidesetting_path %>
