<div class="container fluid-height clearfix">
<%= render 'head'%>
<h1>Listing phone_nums</h1>

<table>
  <thead>
    <tr>
      <th>Contact</th>
      <th>Type</th>
      <th>Is primary</th>
      <th>Attribution</th>
      <th>Operator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phone_nums.each do |phone_num| %>
      <tr>
        <td><%= phone_num.contact_id %></td>
        <td><%= phone_num.type %></td>
        <td><%= phone_num.is_primary %></td>
        <td><%= phone_num.attribution %></td>
        <td><%= phone_num.operator %></td>
        <td><%= link_to 'Show', phone_num %></td>
        <td><%= link_to 'Edit', edit_phone_num_path(phone_num) %></td>
        <td><%= link_to 'Destroy', phone_num, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phone num', new_phone_num_path %>
</div>